<!DOCTYPE html>
<html>
    <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block head_title %}Audience Kit by HyperGrowth | Audience Management Platform - {% block sub_head_title %}{% endblock %}{% endblock %}</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="shortcut icon" type="image/png" href="{{ asset('assets/img/favicon.ico') }}"/>
    {% block stylesheets %}
        <!-- Bootstrap 3.3.5 -->
        <link rel="stylesheet" href="{{ asset('bundles/hyperevent/bootstrap/css/bootstrap.min.css') }}">
        <!-- Font Awesome -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
        <!-- Ionicons -->
        <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
        <!-- Theme style -->
        <link rel="stylesheet" href="{{ asset('bundles/hyperevent/dist/css/style.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/hyperevent/dist/css/animate.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/hyperevent/dist/css/AdminLTE.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/hyperevent/dist/css/skins/_all-skins.min.css') }}">
        <!-- PowerTour core stylesheet(required) -->
        <link rel="stylesheet" href="{{ asset('bundles/hyperevent/plugins/powertour/powertour.3.1.0.css') }}"/>
        <!-- PowerTour styling stylesheet (optional) -->
        <link rel="stylesheet" href="{{ asset('bundles/hyperevent/plugins/powertour/powertour-style-clean.css') }}"/>
        <!-- CSS3 animations stylesheet(needed for animations) -->
        <link rel="stylesheet" href="{{ asset('bundles/hyperevent/plugins/powertour/animate.min.css') }}"/>
        <!-- PowerTour responsive support (optional) -->
        <link rel="stylesheet" href="{{ asset('bundles/hyperevent/plugins/powertour/powertour-mobile.css') }}"/>
        <!-- PowerTour responsive support (optional) -->
        <link rel="stylesheet" href="{{ asset('bundles/hyperevent/plugins/powertour/core-3.css') }}"/>
        <!-- iCheck -->
        <link href="{{ asset('bundles/hyperevent/plugins/iCheck/square/blue.css') }}" rel="stylesheet">
    {% endblock %}
    {% block stylesheets_inline %}{% endblock %}
         <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
            <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>
    <body class="fixed-sidebar">
        <div id="wrapper">
            {{ render(controller('HyperEventBundle:SharedLayout:renderNavbarLeft', {request: app.request})) }}
            <div id="page-wrapper" class="content-wrapper">
                {{ render(controller('HyperEventBundle:SharedLayout:renderNavbarTop', {request: app.request})) }}
                {% block body %}{% endblock %}
                {{ render(controller('HyperEventBundle:SharedLayout:renderUserTutorial', {request: app.request})) }}
                {% include 'layout_ak/_footer.html.twig' %}
                {% include 'layout_ak/_confirm_delete.html.twig' %}
            </div>
        </div>
        {% block javascript_libraries %}
            <!-- jQuery 2.1.4 -->
            <script src="{{ asset('bundles/hyperevent/plugins/jQuery/jQuery-2.1.4.min.js') }}"></script>
            <!-- jQuery UI 1.11.4 -->
            <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
            <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
            <script>
                $.widget.bridge('uibutton', $.ui.button);
            </script>
            <!-- Bootstrap 3.3.5 -->
            <script src="{{ asset('bundles/hyperevent/bootstrap/js/bootstrap.min.js') }}"></script>
            <!-- Slimscroll -->
            <script src="{{ asset('bundles/hyperevent/plugins/slimScroll/jquery.slimscroll.min.js') }}"></script>
            <!-- FastClick -->
            <script src="{{ asset('bundles/hyperevent/plugins/fastclick/fastclick.min.js') }}"></script>
    	    <!-- Metis Menu -->
            <script src="{{ asset('bundles/hyperevent/plugins/metisMenu/jquery.metisMenu.js') }}"></script>
            <!-- AdminLTE App -->
            <script src="{{ asset('bundles/hyperevent/dist/js/app.min.js') }}"></script>
            <!-- jvectormap -->
            <script src="{{ asset('bundles/hyperevent/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js') }}"></script>
            <script src="{{ asset('bundles/hyperevent/plugins/jvectormap/jquery-jvectormap-world-mill-en.js') }}"></script>
    	    <script src="{{ asset('bundles/hyperevent/dist/js/inspinia.js') }}"></script>
    	    <!-- PowerTour plugin (required) -->
            <script src="{{ asset('bundles/hyperevent/plugins/powertour/powertour.3.1.0.min.js') }}"></script>
            <!-- PowerTour Front Page Setting (required) -->
            <script src="{{ asset('bundles/hyperevent/plugins/powertour/frontpage.js') }}"></script>
            <!-- iCheck -->
            <script src="{{ asset('bundles/hyperevent/plugins/iCheck/icheck.min.js') }}"></script>
            <script>
                $('input').iCheck({
        			checkboxClass: 'icheckbox_square-blue',
        			inheritID: true,
            		radioClass: 'iradio_square-blue',
            		increaseArea: '20%'
          		});
          		$('#confirm-delete').on('show.bs.modal', function(e) {
                    $(this).find('.btn-ok').attr('href', $(e.relatedTarget).data('href'));
                });

            </script>
        {% endblock %}
        {% block javascript_inline %}{% endblock %}
    </body>
</html>