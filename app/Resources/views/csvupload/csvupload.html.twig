{% extends 'layout_ak/base.html.twig' %}
{% block sub_head_title %}Admin Dashboard - Upload CSV{% endblock %}
{% block body %}
<h1>Upload Page</h1>
<div class="wrapper wrapper-content animated fadeInRight">
  
  <div class="row">
    <div class="col-md-12">
      {{ form_start(form, {'attr': {'class': 'form-horizontal'} }) }}
        {{ form_errors(form) }}
        {% for flash_message in app.session.flashbag.get('notice') %}
            <div class="flash-notice">
                {{ flash_message }}
            </div>
        {% endfor %}
        <div class="form-group">
          <div class="col-sm-8">
            {{ form_widget(form.file, {'attr': {'class': 'form-control'} }) }}
          </div>
          <div class="col-sm-4"></div>
        </div>
  
        <div class="form-group">
          <div class="col-sm-8">
            {{ form_widget(form.application, {'attr': {'class': 'form-control'} }) }}
          </div>
          <div class="col-sm-4"></div>
        </div>
        
        <div class="form-group">
          <div class="col-sm-8">
            {{ form_widget(form.provider, {'attr': {'class': 'form-control'} }) }}
          </div>
          <div class="col-sm-4"></div>
        </div>
        
        <div class="form-group">
          <div class="col-sm-8">
            {{ form_widget(form.event_type, {'attr': {'class': 'form-control'} }) }}
          </div>
          <div class="col-sm-4"></div>
        </div>
        
        <div class="form-group">
          <div class="col-sm-8">
            {{ form_widget(form.attribute_type, {'attr': {'class': 'form-control'} }) }}
          </div>
          <div class="col-sm-4"></div>
        </div>
        
        <div class="form-group">
          <div class="col-sm-8">
            {{ form_widget(form.save, {'attr': {'class': 'btn btn-danger btn-flat'} }) }}
          </div>
          <div class="col-sm-4"></div>
        </div>
        {{ form_rest(form) }}
  
      {{ form_end(form) }}
    </div>
  </div>
</div>
{% endblock %}